{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "class-methods-use-this": "off",
        "no-restricted-globals": "off",
        "no-plusplus": "off"
    },
    "ignorePatterns": ["assets/js/vendor/", "assets/js/base/"]
}
