<table class="table">
    <tbody>
    <tr>
        <th>Libellé</th>
        <td>{{ etablissement.libelle }}</td>
    </tr>
    <tr>
        <th>Adresse</th>
        <td>{{ etablissement.adresse ? etablissement.adresse.display|raw : 'Non définie' }}</td>
    </tr>
    <tr>
        <th>Calendrier universitaire</th>
        <td>
            {{ etablissement.getEtablissementInformation.getCalendrierUniversitaire 
            ? etablissement.getEtablissementInformation.getCalendrierUniversitaire|raw 
            : 'Non renseigné' 
            }}
        </td>
    </tr>
    <tr>
        <th>Calendrier d'inscription</th>
        <td>
            {{ etablissement.getEtablissementInformation.getCalendrierInscription 
            ? etablissement.getEtablissementInformation.getCalendrierInscription|raw         
            : 'Non renseigné' 
            }}
        </td>
    </tr>
    <tr>
        <th>Informations pratiques</th>
        <td>
            {{ etablissement.getEtablissementInformation.getInformationsPratiques 
                ? etablissement.getEtablissementInformation.getInformationsPratiques|raw 
                : 'Non renseigné' 
            }}
        </td>
    </tr>
    </tbody>
</table>

