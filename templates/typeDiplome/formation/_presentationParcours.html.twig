<div class="row">
    <div class="col-sm-12">
        <dl>
            <dt>Organisation de la formation</dt>
            <dd>
                {{ parcours.contenuFormation|printTexte }}
            </dd>
            {% if stringDifferencesParcours.presentationParcoursContenuFormation is defined %}
            {{ include('communs/_comparaison_parcours_versioning.html.twig', 
                    {
                        textDifferences: stringDifferencesParcours.presentationParcoursContenuFormation,
                        idCollapse: "diffParcoursContenuFormation"
                    }
                ) 
            }}
            {% endif %}
            <dt>Objectifs du parcours</dt>
            <dd>
                {{ parcours.objectifsParcours|printTexte }}
            </dd>
            {% if stringDifferencesParcours.presentationParcoursObjectifsParcours is defined %}
            {{ include('communs/_comparaison_parcours_versioning.html.twig', 
                    {
                        textDifferences: stringDifferencesParcours.presentationParcoursObjectifsParcours,
                        idCollapse: "diffParcoursObjectifs"
                    }
                ) 
            }}
            {% endif %}
            <dt>Résultats attendus du parcours</dt>
            <dd>
                {{ parcours.resultatsAttendus|printTexte }}
            </dd>            
            {% if stringDifferencesParcours.presentationParcoursResultatsAttendus is defined %}
            {{ include('communs/_comparaison_parcours_versioning.html.twig', 
                    {
                        textDifferences: stringDifferencesParcours.presentationParcoursResultatsAttendus,
                        idCollapse: "diffParcoursResultatsAttendus"
                    }
                ) 
            }}
            {% endif %}
            <dt>Rythme de formation</dt>
            <dd>
                {{ parcours.rythmeFormation != null ? parcours.rythmeFormation.libelle : 'Non précisé' }}
            </dd>
            <dt>Précisions sur le rythme de formation</dt>
            <dd>{{ parcours.rythmeFormationTexte|printTexte }}</dd>
        </dl>
    </div>
</div>
