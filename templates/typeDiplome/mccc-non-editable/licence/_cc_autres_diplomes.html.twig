<h3>1ᵉ Session</h3>
<div>
    <table class="table table-bordered">
        <thead>
        <tr>
            <th></th>
            <th>Pourcentage</th>
            <th>CC avec TP</th>
        </tr>
        </thead>
        <tbody>
        {% for mccc in mcccs[1]['cc'] %}
            <tr>

                <th>Contrôle continu N° {{ loop.index }}</th>
                <td>
                    {% if mccc.pourcentage is defined %}
                        {{ mccc.pourcentage }}%
                    {% else %}
                        -%
                    {% endif %}
                </td>
                <td>
                    {% if mccc.options is defined and mccc.options.cc_has_tp is defined and mccc.options.cc_has_tp == "on" %}
                        Oui
                    {% else %}
                        Non
                    {% endif %}
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>




<h3 class="mt-2">2ᵉ Session</h3>

    {% if mcccs[2]['et'] is defined %}
        {{ include('typeDiplome/mccc-non-editable/licence/_secondeChance.html.twig', 
            {
                mcccs:mcccs[2]['et'],
                mcccVersioning: mcccVersioning[2]['et'] ?? ""
            }) 
        }}
    {% endif %}



