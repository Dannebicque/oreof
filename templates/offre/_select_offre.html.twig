    {% if is_granted('ROLE_ADMIN') %}
        <option value="FERMETURE_DEFINITIVE" {% if valeur == 'FERMETURE_DEFINITIVE' %}selected{% endif %}>fermeture
            d√©finitive
        </option>
    {% endif %}
