{% if is_granted('ROLE_ADMIN') %}
    {% if stringData is defined 
        and stringData != null
        and stringData|length > 0
    %}
        <a data-bs-toggle="collapse" data-bs-target="#{{idCollapse}}" role="button">
            <span class="badge rounded-pill text-bg-primary mt-2 mb-3">
                Voir les diff√©rences
            </span>
        </a>
        <div class="row mb-4 collapse" id="{{idCollapse}}">
            <div class="col-8">
                <div class="d-inline-flex">
                    {{ stringData|raw }}  
                </div>
            </div> 
        </div>
    {% endif %}
{% endif %}